ROOT_UID=0             # Root has $UID 0.
E_ANY_ERROR=1	       # Catchall for general errors
E_BASH_SHELL=2	       # Bash script error
E_CMD_CALL=126	       # Common invoked cannot execute
E_CMD_ABSENT=127       # Command not found
E_BADARG_EXIT=128      # Bad argument, need to exit
E_CTRL_C_EXIT=130      # Script terminated by Ctrl C
E_NOT_ROOT=101         # Not root user error.
E_BAD_INPUT=85         # Bad input value

SUCCESS=0
FAILURE=-1	       # Unknown Exit Error

echo "Imported Error Code Table"
